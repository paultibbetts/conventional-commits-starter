{
  "name": "@ptibbetts/conventional-commits-starter",
  "version": "1.0.2",
  "description": "Starter package to get you up and running with conventional commits",
  "main": "dist/.huskyrc",
  "scripts": {
    "install:peerdeps": "npx install-peerdeps @ptibbetts/conventional-commits-starter --only-peers",
    "install:commitizen": "commitizen init cz-conventional-changelog --save-dev --save-exact",
    "install:husky": "cp -u ./dist/.huskyrc $INIT_CWD",
    "postinstall": "! [ $npm_config_save ] && npm-run-all install:* || echo 'not running @ptibbetts/conventional-commits-starter postinstall'"
  },
  "keywords": [
    "git"
  ],
  "author": "Paul Tibbetts <code@paultibbetts.uk>",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
