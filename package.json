{
  "name": "git",
  "version": "1.0.0",
  "description": "My JavaScript Git setup",
  "main": "index.js",
  "scripts": {
    "install:peerdeps": "npx install-peerdeps ptibbetts/git",
    "install:commitizen": "commitizen init cz-conventional-changelog --save-dev --save-exact",
    "install:husky": "cp -u ./dist/.huskyrc $INIT_CWD",
    "postinstall": "npm-run-all install:*"
  },
  "keywords": [
    "git"
  ],
  "author": "Paul Tibbetts <code@paultibbetts.uk>",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
