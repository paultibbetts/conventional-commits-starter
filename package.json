{
  "name": "@ptibbetts/conventional-commits-starter",
  "version": "1.1.0",
  "description": "Starter package to get you up and running with conventional commits",
  "main": "dist/.huskyrc",
  "scripts": {
    "install": "sh scripts/install-peerdeps.sh && sh scripts/huskyrc.sh && sh scripts/commitizen.sh"
  },
  "keywords": [
    "git"
  ],
  "author": "Paul Tibbetts <code@paultibbetts.uk>",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptibbetts/conventional-commits-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ptibbetts/conventional-commits-starter/issues"
  },
  "homepage": "https://github.com/ptibbetts/conventional-commits-starter#readme"
}
