{
  "name": "@ptibbetts/conventional-commits-starter",
  "version": "1.1.4",
  "description": "Starter package to get you up and running with conventional commits",
  "main": "dist/.huskyrc",
  "scripts": {
    "postinstall": "bash scripts/postinstall.sh $INIT_CWD"
  },
  "keywords": [
    "git"
  ],
  "author": "Paul Tibbetts <code@paultibbetts.uk>",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "peerDependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "semantic-release": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptibbetts/conventional-commits-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ptibbetts/conventional-commits-starter/issues"
  },
  "homepage": "https://github.com/ptibbetts/conventional-commits-starter#readme"
}
