{
  "name": "@ptibbetts/conventional-commits-starter",
  "version": "1.2.4",
  "description": "Starter package to get you up and running with Conventional Commits",
  "main": "dist/index.js",
  "bin": {
    "conventional-commits-starter": "dist/index.js"
  },
  "scripts": {
    "prestart": "$npm_execpath run build",
    "start": "node dist/index.js",
    "build": "tsc",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "git",
    "commit",
    "conventional-commits",
    "semantic-release",
    "conventional-changelog"
  ],
  "author": "Paul Tibbetts <code@paultibbetts.uk>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "commitizen": "^4.1.2",
    "husky": "^4.2.5",
    "js-beautify": "^1.11.0",
    "ora": "^5.0.0",
    "yargs": "^15.4.1"
  },
  "peerDependencies": {
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/js-beautify": "^1.11.0",
    "@types/node": "^14.0.27",
    "@types/yargs": "^15.0.5",
    "cz-conventional-changelog": "^3.2.0",
    "semantic-release": "^17.1.1",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptibbetts/conventional-commits-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ptibbetts/conventional-commits-starter/issues"
  },
  "homepage": "https://github.com/ptibbetts/conventional-commits-starter#readme"
}
